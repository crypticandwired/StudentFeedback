    "build:server": "echo Server has no build step",
    "dev": "concurrently -k -n CLIENT,SERVER -c green,blue \"npm:dev:client\" \"npm:dev:server\"",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "lint": "next lint",
    "start": "concurrently -k -n CLIENT,SERVER -c green,blue \"npm:start:client\" \"npm:start:server\"",
